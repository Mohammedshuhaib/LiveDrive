                <div class='circle'>
                  <p class='pt-2' style='padding-left:15px ;'>{{this.booking.Days}}d</p>
                </div>
              </div>
            </div>
            <div>
              <p class='pick-drop text-dark'>DROP</p>
              <p class='pick-drop-date text-dark'>DATE</p>
              <p class='enterDate text-danger'>{{this.booking.dropDate}}</p>
            </div>
          </div>
          <div>
            <p>Payment details</p>
            <hr style='margin-top: -10px;height: 1px ; background-color: black;' />
          </div>
          <div class='d-flex text-dark booking-content'>
            <div>
              <span class='d-flex'>
                <p>Total amount : </p>
                <p class='pl-3'>RS:{{this.booking.Rent}}</p>
              </span>
            </div>
            <div class='ml-auto booking-content'>
              <span class='d-flex'>
                <p class='mr-4'>Payment status:</p><a href='' class='btn btn-secondary mb-2'>{{this.booking.status}}</a>
              </span>
            </div>
          </div>
 <hr style='margin-top: 10px;height: 1px ; background-color: black;' />
        </div>
           {{/each}}
           
      </div>
 
       
      {{! upcoming end }}
      {{! compleatd start }}
      <div class='tab-pane fade show' id='compleated-md' role='tabpanel' aria-labelledby='compleated-tab-md'>
        {{#each compleatedBooking}}
        <div class='d-flex'>
          <div>
            <img class='booking-img' src='/uploads/{{this.booking.vehicleImage}}' height='120px' alt='' />
          </div>
          <div class='mt-4 px-3'>
            <p class='booking_main_text'>{{this.booking.Name}}</p>
